{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "leverage.schema.json",
    "title": "leverage",
    "description": "leverage json schema",
    "type": "object",
    "properties": {
        "botId": {
            "description": "Unique identifier for the bot",
            "type": "string"
        },
        "symbol": {
            "description": "Symbol to modify the leverage on",
            "type": "string"
        },
        "leverage": {
            "description": "New leverage for the symbol",
            "type": "number"
        }
    },
    "required": [
        "symbol",
        "leverage"
    ],
    "additionalProperties": false
}
