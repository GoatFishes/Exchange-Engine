{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "keyBotUpload.schema.json",
    "title": "keyBotUpload",
    "description": "keyBotUpload json schema",
    "type": "object",
    "properties": {
        "botId": {
            "description": "unique identifier for the bot",
            "type": "string"
        },
        "apiKeyId": {
            "description": "API key id for the exchange we want to execute the code for",
            "type": "string"
        },
        "apiKeySecret": {
            "description": "API key secret for the exchange we want to execute the code for",
            "type": "string"
        },
        "exchange": {
            "description": "Exchange the keys belong to",
            "type": "string"
        }
    },
    "required": [
        "botId",
        "apiKeyId",
        "apiKeySecret",
        "exchange"
    ],
    "additionalProperties": false
}
